@import 'includes/examples'

p {
    margin: 50px;
    padding: 0;
}

img {
    width: 100%;
}

.photo {
    width: 300px;
}

.example {
    border-color: #ffbe5c;
    border-image: url(http://hara-css3.herokuapp.com/img/border.png) 27 / 27px / 20px repeat;
    border-style: solid;
    border-width: 27px;
}

.frame1 {
    border-image: url(http://hara-css3.herokuapp.com/img/border.png) 27 27 repeat;
    border-width: 27px;
}

.frame {
    border-image: url(http://hara-css3.herokuapp.com/img/icon_flower.jpg) 100% round stretch;
    border-style: solid;
    border-width: 15px;
    width: 300px;
}



@import 'includes/examples'

#main
	border 1px solid #000
	box-sizing border-box
	margin 10px 0 0 10px
	padding 35px 0
	width 210px
	a
		color #f8f8f8
		display block
		font-size 2em
		text-decoration none
		max-width 3em
		text-align center
		padding 10px 15px
		margin 0 auto

.button
	background #06f
	background linear-gradient(#06f, #03f)
	border 1px solid #06c
	border-radius 2px
	box-shadow 0 0 10px rgba(0,0,0,.2)
	text-shadow 1px 1px 0 #03c
